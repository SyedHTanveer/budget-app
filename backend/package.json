{
  "name": "budget-app-backend",
  "version": "1.0.0",
  "description": "AI-powered budgeting assistant backend",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "migrate": "knex migrate:latest",
    "migrate:make": "knex migrate:make",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "db:setup": "npm run migrate && npm run seed",
    "azure:migrate": "NODE_ENV=production knex migrate:latest",
    "azure:seed": "NODE_ENV=production knex seed:run",
    "azure:setup": "npm run azure:migrate && npm run azure:seed",
    "azure:deploy": "npm run azure:setup && npm start",
    "setup": "npm run migrate && npm run seed",
    "reset": "npm run migrate:rollback && npm run migrate && npm run seed",
    "db:status": "knex migrate:currentVersion",
    "local": "LOCAL_ONLY_MODE=true nodemon src/index.js",
    "test": "jest --runInBand"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^6.8.1",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^2.5.1",
    "openai": "^4.0.0",
    "pg": "^8.11.1",
    "plaid": "^12.0.0",
    "sqlite3": "^5.1.6",
    "stripe": "^14.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.10.0",
    "pino": "^9.0.0",
    "pino-pretty": "^11.0.0",
    "zod": "^3.23.0",
    "cookie-parser": "^1.4.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
